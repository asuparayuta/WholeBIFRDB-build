    "# テーブル別に「NOT NULL の必須カラム」と「空時の既定値」を定義\n",
    "REQUIRED = {\n",
    "    # ---------- project ----------\n",
    "    \"project\": {\n",
    "        \"project_id\":  lambda r: r[\"project_id\"],           # 主キーそのもの\n",
    "        \"contributor\": lambda r: \"(auto)\",                  # 無ければ自動\n",
    "        \"bra_version\": lambda r: \"NA\"                       # 無ければ \"NA\"\n",
    "    },\n",
    "\n",
    "    # ---------- references_tbl ----------\n",
    "    \"references_tbl\": {\n",
    "        \"reference_id\": lambda r: r[\"reference_id\"],        # PK\n",
    "        \"doi\":    lambda r: \"NO_DOI\",                       # 空 → NO_DOI\n",
    "        \"bibtex\": lambda r: \"NO_BIBTEX\"                     # 空 → NO_BIBTEX\n",
    "    },\n",
    "\n",
    "    # ---------- circuits ----------\n",
    "    \"circuits\": {\n",
    "        \"circuit_id\":   lambda r: r[\"circuit_id\"],          # PK\n",
    "        \"source_of_id\": lambda r: \"UNKNOWN_SRC\",            # 空 → 既定値\n",
    "        \"names\":        lambda r: f\"UNKNOWN_NAME_{r['circuit_id']}\",\n",
    "        \"project_id\":   lambda r: f\"AUTO_PROJECT_{r['circuit_id']}\"  # 空 → 自動\n",
    "    },\n",
    "\n",
    "    # ---------- connections ----------\n",
    "    \"connections\": {\n",
    "        \"sender_circuit_id\":   lambda r: r[\"sender_circuit_id\"],\n",
    "        \"receiver_circuit_id\": lambda r: r[\"receiver_circuit_id\"],\n",
    "        \"reference_id\":        lambda r: r[\"reference_id\"]\n",
    "        # 他のカラムは NOT NULL ではない想定\n",
    "    },\n",
    "\n",
    "    # ---------- settings ----------\n",
    "    \"settings\": {\n",
    "        \"wholebif_file_id\": lambda r: r[\"wholebif_file_id\"]   # PK\n",
    "    }\n",
    "}"
