    "def circuit_exists(conn, cid: str) -> bool:\n",
    "    \"\"\"cid が circuits に存在するか True/False を返す\"\"\"\n",
    "    with conn.cursor() as cur:\n",
    "        cur.execute(\"SELECT 1 FROM circuits WHERE circuit_id = %s;\", (cid,))\n",
    "        return bool(cur.fetchone())\n",
    "\n",
    "def log_missing_circuit(conn, cid: str, ref_id: str):\n",
    "    \"\"\"\n",
    "    circuits に無い sender / receiver を検出したときに\n",
    "    changelog へ WARNING 行を残す（INSERT 失敗はしない）\n",
    "    \"\"\"\n",
    "    detail = f\"Connections に {cid} が出現したが circuits に存在しない (Reference={ref_id})\"\n",
    "    log_change(conn, \"connections\", cid, \"WARN\", detail)"
   ]
